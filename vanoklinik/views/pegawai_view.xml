<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <!-- vanoklinik.pegawai tree view -->
    <record id="vanoklinik_pegawai_view_tree" model="ir.ui.view">
        <field name="name">vanoklinik.pegawai.view.tree</field>
        <field name="model">vanoklinik.pegawai</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="alamat"/>
                <field name="j_kelamin"/>
                <field name="tgl_lahir"/> 
            </tree>
        </field>
    </record>
    <!-- vanoklinik.pegawai form view -->
    <record id="vanoklinik_pegawai_view_form" model="ir.ui.view">
        <field name="name">vanoklinik.pegawai.view.form</field>
        <field name="model">vanoklinik.pegawai</field>
        <field name="arch" type="xml">
            <form string="Data Diri">
            <sheet>
                <group name="datadiri" string="Data Diri">
                    <field name="name"/>
                    <field name="alamat"/>
                    <field name="tgl_lahir"/>
                    <field name="j_kelamin" required="1"/>
                </group>
            </sheet>
            </form>
        </field>
    </record>
</odoo>
